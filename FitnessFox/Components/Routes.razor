@using FitnessFox.Components.Components.Account.Shared
@using FitnessFox.Components.Components
<Router AppAssembly="typeof(Program).Assembly" AdditionalAssemblies="new[]{typeof(FitnessFox.Components.Components.Layout.MainLayout).Assembly}">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(Components.Layout.MainLayout)" >
            <NotAuthorized>
                <RedirectToLogin />
            </NotAuthorized>
        </AuthorizeRouteView>
        <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Found>
</Router>
